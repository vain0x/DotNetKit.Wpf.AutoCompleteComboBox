<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net462;net8-windows</TargetFrameworks>
    <UseWPF>true</UseWPF>
    <RootNamespace>DotNetKit</RootNamespace>
    <AssemblyName>DotNetKit.Wpf.AutoCompleteComboBox</AssemblyName>
    <Description>Provides a lightweight combobox with filtering (auto-complete).</Description>
    <Copyright>Copyright (c) 2017 vain0</Copyright>
    <Version>2.0.0-dev</Version>
    <Authors>vain0</Authors>
    <projectUrl>https://github.com/vain0x/DotNetKit.Wpf.AutoCompleteComboBox</projectUrl>
    <licenseUrl>http://opensource.org/licenses/MIT</licenseUrl>
    <PackageTags>Wpf Combobox AutoComplete</PackageTags>
    <PackageProjectUrl>https://github.com/vain0x/DotNetKit.Wpf.AutoCompleteComboBox</PackageProjectUrl>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Release'">
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>
